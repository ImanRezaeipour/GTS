      <RemainLeave Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>RemainLeave</Alias>
        <Columns isList="true" count="17">
          <value>prs_FirstName,System.String</value>
