  --declare @p0 datetime,@p1 datetime,@p2 decimal(28,5)
--set @p0='2012-03-21 00:00:00'
--set @p1='2014-03-20 00:00:00'
--set @p2=47746.00000
  
  
--declare @fromDate datetime,@toDate datetime

--set @fromDate=@p0
--set @toDate=@p1


SELECT Number, ID,mngrFlow_ManagerID ManagerID,request_PrecardID PrecardID,request_PersonID PersonID,
dbo.GTS_ASM_MiladiToShamsi( cast( FromDate as date)) FromDate,dbo.GTS_ASM_MiladiToShamsi( cast( ToDate as date)) ToDate,FromTime,ToTime,
TimeDuration,Description,dbo.GTS_ASM_MiladiToShamsi( cast( RegisterDate as date)) RegisterDate,UserID,AttachmentFile,
OperatorUser,mngrFlowID
,Precrd_Name PrecardName,Precrd_Hourly IsHourly,Precrd_Daily IsDaily, Precrd_Monthly IsMonthly, Prs_FirstName ApplicantFirstName , Prs_LastName ApplicantLastName,Prs_Barcode PersonCode, prs.PrsDtl_Image PersonImage
, precardGroup.PishcardGrp_LookupKey LookupKey
, (select top(1) prs_barcode from ta_person where prs_id in( select MasterMng_PersonID from TA_Manager where MasterMng_ID=mngrFlow_ManagerID)) PrsBarcode1
, (select top(1) prs_firstname+' '+prs_lastname from ta_person where prs_id in( select MasterMng_PersonID from TA_Manager where MasterMng_ID=mngrFlow_ManagerID)) PrsName1
, (select top(1) prs_barcode from ta_person where prs_id in(select organ_personid from TA_OrganizationUnit where organ_id in( select MasterMng_OrganizationUnitID from TA_Manager where MasterMng_ID=mngrFlow_ManagerID))) PrsBarcode2
, (select top(1) prs_firstname+' '+prs_lastname from ta_person where prs_id in(select organ_personid from TA_OrganizationUnit where organ_id in( select MasterMng_OrganizationUnitID from TA_Manager where MasterMng_ID=mngrFlow_ManagerID))) PrsName2
, dbo.GTS_ASM_MiladiToShamsi( cast( @fromDate as date)) FD
, dbo.GTS_ASM_MiladiToShamsi( cast( @toDate as date)) TD
 FROM
(
SELECT ROW_NUMBER() Over(order by request_FromDate,request_FromTime,request_ID) as Number,MngFlow.mngrFlow_ManagerID, request_ID ID,request_PrecardID PrecardID,request_PersonID PersonID ,
request_FromDate FromDate,request_ToDate ToDate,request_FromTime FromTime,request_ToTime ToTime, request_AttachmentFile AttachmentFile,
isnull(request_TimeDuration,0) TimeDuration,isnull(request_Description,'''') Description,request_RegisterDate RegisterDate,request_UserID UserID,
isnull(request_OperatorUser,'''') as OperatorUser,
MngFlow.mngrFlow_ID mngrFlowID,request_PersonID
,request_PrecardID
FROM (SELECT * FROM TA_ManagerFlow where mngrFlow_Active=1  AND mngrFlow_ManagerID in ( --------------------
select m.MasterMng_ID from ta_manager m
join TA_ManagerFlow mf on m.MasterMng_ID=mf.mngrFlow_ManagerID
join ta_flow f on f.Flow_ID=mf.mngrFlow_FlowID
left join TA_OrganizationUnit ou on ou.organ_ID = m.MasterMng_OrganizationUnitID
where f.Flow_MainFlow=1 and m.MasterMng_Active=1 and mf.mngrFlow_Active=1  and f.Flow_ActiveFlow=1
and (m.MasterMng_PersonID in ({ReportHelper.Instance().ModifiedIds()}) or ou.organ_PersonID in ({ReportHelper.Instance().ModifiedIds()}))
) ----------------------------------------------------------------------------------------------------------
      ) MngFlow
INNER JOIN (SELECT Flow.Flow_ID, PrcAccessGrpDtl.accessGrpDtl_PrecardId,Flow_FlowName
	FROM TA_Flow Flow
			INNER JOIN TA_PrecardAccessGroupDetail  PrcAccessGrpDtl
			ON PrcAccessGrpDtl.accessGrpDtl_AccessGrpId = Flow.Flow_AccessGroupID			
		   ) AS UnderMgn
ON MngFlow.mngrFlow_FlowID = UnderMgn.Flow_Id							
CROSS APPLY [dbo].[TA_GetUnderManagmentPersons] (UnderMgn.Flow_ID) as UndermanagmentsPersons						
INNER JOIN	(SELECT request_ID,request_FromDate,request_FromTime,request_ToDate,request_ToTime,request_TimeDuration,request_RegisterDate,request_PersonID ,request_PrecardID
	,request_Description,request_UserID,request_OperatorUser, request_AttachmentFile
	FROM TA_Request             
	WHERE ( request_FromDate &gt;= @fromDate
                    AND 
                   request_FromDate &lt;= @toDate )
                   OR
                   ( request_ToDate &gt;= @fromDate
                    AND 
                   request_ToDate &lt;= @toDate )
            ) Req
ON Req.request_PersonID = UndermanagmentsPersons.prs_id				
    AND
   Req.request_PrecardID = UnderMgn.accessGrpDtl_PrecardId


WHERE (Req.request_ID IN (SELECT isnull(ReqSts.reqStat_RequestID,0)
	FROM TA_RequestStatus ReqSts
INNER JOIN (SELECT mngrFlow_ID 
	FROM TA_ManagerFlow 
	WHERE mngrFlow_FlowID = MngFlow.mngrFlow_FlowID
	AND
	mngrFlow_Level = MngFlow.mngrFlow_Level - 1
						             ) InnerMngFlow
		                  ON ReqSts.reqStat_MnagerFlowID = InnerMngFlow.mngrFlow_ID					 							   
				                AND
			                 ReqSts.reqStat_Confirm = 1
						 )
         OR
       MngFlow.mngrFlow_Level = 1
      )
         AND
Req.request_ID NOT IN (SELECT isnull(reqStat_RequestID,0)
	FROM TA_RequestStatus 
	WHERE reqStat_RequestId = Req.request_ID AND
	reqStat_MnagerFlowID = MngFlow.mngrFlow_ID 
			                )
        AND
req.request_ID NOT IN (SELECT isnull(reqStat_RequestID,0)
	FROM TA_RequestStatus 
			                 JOIN TA_ManagerFlow as otherMngFlw on otherMngFlw.mngrFlow_ID=reqStat_MnagerFlowID
								    WHERE reqStat_RequestId = Req.request_ID 
									AND otherMngFlw.mngrFlow_FlowID &lt;&gt; MngFlow.mngrFlow_FlowID
			                  )
AND/*changed manager flow*/
req.request_ID NOT IN (SELECT isnull(reqStat_RequestID,0)
	FROM TA_RequestStatus 
	Where reqStat_RequestId = Req.request_ID AND
	reqStat_EndFlow = 1 
			                  )	
			) as result1
INNER JOIN (Select TA_Person.*, TA_PersonDetail.PrsDtl_Image from TA_Person inner join TA_PersonDetail  on TA_Person.Prs_PrsDtlID = TA_PersonDetail.PrsDtl_ID) prs on prs.Prs_ID=request_PersonID AND prs.prs_IsDeleted=0
                        INNER JOIN TA_Precard on Precrd_ID=result1.request_PrecardID   
                        INNER JOIN TA_PrecardGroups as precardGroup on precardGroup.PishcardGrp_ID=Precrd_pshcardGroupID</SqlCommand>
      </DataSource1>
