	,Prs_LastName
	,Prs_BarCode	
	,ScndCnpValue_PeriodicFromDate
	,ScndCnpValue_PeriodicToDate
	,resultCalcValidation 
	FROM (							        
SELECT ISNULL(SUM([gridFields_NecessaryOperation]), -1000) [gridFields_NecessaryOperation]
      ,ISNULL(SUM([gridFields_HourlyPureOperation]), -1000) [gridFields_HourlyPureOperation]
      ,ISNULL(SUM([gridFields_DailyPureOperation]), -1000) [gridFields_DailyPureOperation]
      ,ISNULL(SUM([gridFields_ImpureOperation]), -1000) [gridFields_ImpureOperation]
      ,ISNULL(SUM([gridFields_AllowableOverTime]), -1000) [gridFields_AllowableOverTime]
      ,ISNULL(SUM([gridFields_UnallowableOverTime]), -1000) [gridFields_UnallowableOverTime]
      ,ISNULL(SUM([gridFields_HourlyAllowableAbsence]), -1000) [gridFields_HourlyAllowableAbsence]
      ,ISNULL(SUM([gridFields_HourlyUnallowableAbsence]), -1000) [gridFields_HourlyUnallowableAbsence]
      ,ISNULL(SUM([gridFields_DailyAbsence]), -1000) [gridFields_DailyAbsence]
