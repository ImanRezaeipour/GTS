      <PersonnelAbsence Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>PersonnelAbsence</Alias>
        <Columns isList="true" count="16">
          <value>prs_FirstName,System.String</value>
          <value>prs_LastName,System.String</value>
          <value>prs_barcode,System.Decimal</value>
          <value>gridFields_HourlyAllowableAbsence,System.Decimal</value>
          <value>gridFields_HourlyUnallowableAbsence,System.Decimal</value>
          <value>gridFields_DailyAbsence,System.Decimal</value>
          <value>gridFields_HourlyUnallowableDelay,System.Decimal</value>
          <value>gridFields_HourlyUnallowableAcceleration,System.Decimal</value>
          <value>gridFields_HourlyBetweenTimeUnallowableAbsence,System.Decimal</value>
          <value>ShamsiDate,System.String</value>
          <value>ScndCnpValue_PersonId,System.Decimal</value>
          <value>gridFields_HourlyallowableDelay,System.Decimal</value>
          <value>miladiDate,System.DateTime</value>
          <value>fromDateSelected,System.DateTime</value>
          <value>toDateSelected,System.DateTime</value>
          <value>gridFields_PureHourlyUnallowableAbsence,System.Decimal</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>PersonnelAbsence</Name>
        <NameInSource>Connection</NameInSource>
        <Parameters isList="true" count="2">
          <value>toDate,,4,0</value>
          <value>fromDate,,4,0</value>
        </Parameters>
        <SqlCommand>SELECT 
	gridFields_HourlyAllowableAbsence
	
	,gridFields_HourlyUnallowableDelay
	,gridFields_DailyAbsence
	,dbo.GTS_ASM_MiladiToShamsi(CONVERT(nvarchar(10), [Date], 111)) AS ShamsiDate
	,([Date]) as miladiDate
	,ScndCnpValue_PersonId
	,prs_barcode
	,prs_firstName
	,prs_LastName
	,(@fromDate) as fromDateSelected
	,(@toDate) as toDateSelected
FROM (SELECT ScndCnpValue_FromDate [Date]
	    ,ISNULL(SUM([gridFields_HourlyAllowableAbsence]), -1000) [gridFields_HourlyAllowableAbsence]
      ,ISNULL(SUM([gridFields_HourlyUnallowableAbsence]), -1000) [gridFields_HourlyUnallowableAbsence]
       ,ISNULL(SUM([gridFields_PureHourlyUnallowableAbsence]), -1000) [gridFields_PureHourlyUnallowableAbsence]
      ,ISNULL(SUM([gridFields_DailyAbsence]), -1000) [gridFields_DailyAbsence]
      ,ISNULL(SUM([gridFields_HourlyUnallowableDelay]), -1000) [gridFields_HourlyUnallowableDelay]
      ,ISNULL(SUM([gridFields_HourlyallowableDelay]), -1000) [gridFields_HourlyallowableDelay]
      ,ISNULL(SUM([gridFields_HourlyUnallowableAcceleration]), -1000) [gridFields_HourlyUnallowableAcceleration]
      ,ISNULL(SUM([gridFields_HourlyBetweenTimeUnallowableAbsence]), -1000) [gridFields_HourlyBetweenTimeUnallowableAbsence]
      ,ScndCnpValue_PersonId
      
FROM (SELECT ScndCnpValues.ScndCnpValue_ID,
	CnpTmp.ConceptTmp_FnName,
	CnpTmp.ConceptTmp_EngName,
	CnpTmp.ConceptTmp_KeyColumnName,		
	ScndCnpValues.ScndCnpValue_FromDate,
	ScndCnpValues.ScndCnpValue_ToDate,
	ScndCnpValues.ScndCnpValue_Value,
	ScndCnpValue_PersonId
	FROM TA_ConceptTemplate CnpTmp
INNER JOIN (SELECT ScndCnpValue_ID, 
	ScndCnpValue_SecondaryConceptId, 
	ScndCnpValue_PersonId,
	ScndCnpValue_Value,
	ScndCnpValue_FromPairs,
	ScndCnpValue_ToPairs,
	ScndCnpValue_FromDate,
	ScndCnpValue_ToDate							   
	FROM TA_SecondaryConceptValue) AS  ScndCnpValues
						ON CnpTmp.ConceptTmp_ID = ScndCnpValue_SecondaryConceptId
			WHERE ScndCnpValue_PersonId IN ({ReportHelper.Instance().ModifiedIds()})
					AND
				  ScndCnpValue_FromDate &gt;= @FromDate
					AND
				  ScndCnpValue_ToDate &lt;= @ToDate 
				    AND 
				  CnpTmp.ConceptTmp_IsPeriodic = 0
				    AND
				  ScndCnpValues.ScndCnpValue_Value &lt;&gt; 0  
	) AS [Source]		
PIVOT
(
	SUM(ScndCnpValue_Value)
	FOR ConceptTmp_KeyColumnName
		IN ([gridFields_HourlyAllowableAbsence]
			  ,[gridFields_HourlyUnallowableAbsence]
			  ,[gridFields_PureHourlyUnallowableAbsence]
			  ,[gridFields_DailyAbsence]
			  ,[gridFields_HourlyUnallowableDelay]
			  ,[gridFields_HourlyallowableDelay]
			  ,[gridFields_HourlyUnallowableAcceleration]
			  ,[gridFields_HourlyBetweenTimeUnallowableAbsence]
			  )
			  
) AS Result			
GROUP BY ScndCnpValue_PersonId, ScndCnpValue_FromDate 
) ScndCnpValue
INNER JOIN TA_Person Prs
ON ScndCnpValue.ScndCnpValue_PersonId = Prs.Prs_ID
WHERE [gridFields_HourlyAllowableAbsence] &lt;&gt; -1000
		OR 
	  [gridFields_HourlyUnallowableAbsence] &lt;&gt; -1000
		OR
	  [gridFields_PureHourlyUnallowableAbsence] &lt;&gt; -1000
		OR
	  [gridFields_DailyAbsence] &lt;&gt; -1000
		OR
	  [gridFields_HourlyUnallowableDelay] &lt;&gt; -1000
		OR
	  [gridFields_HourlyallowableDelay] &lt;&gt; -1000
		OR
	  [gridFields_HourlyUnallowableAcceleration] &lt;&gt; -1000
		OR
	  [gridFields_HourlyBetweenTimeUnallowableAbsence] &lt;&gt; -1000</SqlCommand>
      </PersonnelAbsence>
